{
  "parser": "@typescript-eslint/parser",

  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": ["prettier/@typescript-eslint", "plugin:prettier/recommended"],
  "rules": {
    "no-console": "off",
    "no-alert": "warn",
    "no-debugger": "warn"
  }
}
